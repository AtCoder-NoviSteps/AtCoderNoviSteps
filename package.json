{
  "name": "atcoder-novisteps",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "bugs": "https://github.com/AtCoder-NoviSteps/AtCoderNoviSteps/issues",
  "homepage": "",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm format",
      "pnpm lint"
    ]
  },
  "scripts": {
    "dev": "vite dev",
    "build": "prisma generate && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "postinstall": "prisma generate",
    "db:seed": "pnpm dlx vite-node ./prisma/seed.ts",
    "db:studio": "pnpm dlx prisma studio",
    "prepare": "husky install",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "1.40.1",
    "@quramy/prisma-fabbrica": "2.0.2",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/adapter-vercel": "3.1.0",
    "@sveltejs/kit": "1.30.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "flowbite-svelte": "0.44.22",
    "flowbite-svelte-icons": "0.4.4",
    "husky": "^8.0.0",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "prettier-plugin-svelte": "3.1.2",
    "prettier-plugin-tailwindcss": "0.5.11",
    "prisma": "5.8.1",
    "svelte": "4.2.8",
    "svelte-check": "3.6.3",
    "sveltekit-superforms": "1.13.3",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "4.5.1",
    "vitest": "0.34.6",
    "zod": "3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-prisma": "3.0.2",
    "@popperjs/core": "2.11.8",
    "@prisma/client": "5.8.1",
    "@testing-library/svelte": "^4.0.5",
    "@types/jest": "^29.5.11",
    "autoprefixer": "10.4.16",
    "flowbite": "2.2.1",
    "lucia": "2.7.6",
    "playwright": "^1.40.1",
    "pnpm": "8.14.1",
    "svelte-add": "2023.9.17-0.0",
    "tailwind-merge": "1.14.0",
    "tailwindcss": "3.4.1",
    "vercel": "33.1.0"
  }
}
